generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Colaborador {
  uuid    String    @id @default(uuid())
  nome    String
  CPF     String @unique
  email   String @unique

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}